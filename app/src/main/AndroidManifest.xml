<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.arpangroup.apps.quizme">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name="com.arpangroup.apps.quizme.SpinnerActivity"></activity>
        <activity android:name="com.arpangroup.apps.quizme.ResultActivity"></activity>
        <activity android:name="com.arpangroup.apps.quizme.QuizActivity"></activity>
        <activity android:name="com.arpangroup.apps.quizme.SignupActivity"></activity>
        <activity android:name="com.arpangroup.apps.quizme.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.arpangroup.apps.quizme.MainActivity"></activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>